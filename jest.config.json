{
  "testEnvironment": "node",
  "testPathIgnorePatterns": ["/node_modules/"],
  "collectCoverageFrom": ["src/**/*.js"],
  "coveragePathIgnorePatterns": ["/node_modules/"],
  "coverageThreshold": {
    "global": {
      "branches": 40,
      "functions": 40,
      "lines": 40,
      "statements": 40
    }
  }
}
